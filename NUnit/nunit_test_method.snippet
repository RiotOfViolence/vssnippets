<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>nunittest</Title>
			<Shortcut>nutest</Shortcut>
			<Description>Code snippet for empty NUnit test method</Description>
			<Author>Adrian Kuper</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>MethodToTest_TestCase</Default>
					<ToolTip>Name of test method</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[[Test]
			public void $name$()
			{
				// ------------------------------------------------------------
				// PREPARE
				// ------------------------------------------------------------
			$selected$ $end$
			
				// ------------------------------------------------------------
				// SYSTEM UNDER TEST
				// ------------------------------------------------------------

				// ------------------------------------------------------------
				// VERIFY
				// ------------------------------------------------------------
				Assert.Inconclusive();
			}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>